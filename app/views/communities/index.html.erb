<div class="container-fluid restaurants-row">
  <div id="food-row" class="row">
    <% @communities.each do |community| %>
      <div class="col-md-2 col-xs-6">
        <div class="food-item">
          <div class="food-image">
            <%= link_to image_tag(community.image.url(:small), class: "food-image-tag") %>
          </div>
          <div class="food-text">
            <%= link_to community.name, community_path(community) %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
