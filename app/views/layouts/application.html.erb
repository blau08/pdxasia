<!DOCTYPE html>
<html>
<head>
  <title>PDXAsia</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro" rel="stylesheet">
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav class="navbar-social">
    <div class="container">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="social-links">
        <ul>
          <li><a href="#"><i class="fa fa-facebook-square" aria-hidden="true"> /PDXAsia</i></a></li>
          <li><a href="#"><i class="fa fa-instagram" aria-hidden="true"> /PDXAsia</i></a></li>
          <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"> /PDXAsia</i></a></li>
        </ul>

      </div>
    </div><!-- /.container-fluid -->
  </nav>

<%= yield %>

</body>
</html>
