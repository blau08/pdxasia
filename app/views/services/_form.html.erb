<%= render "layouts/errors", :object => @service %>

<%= form_for @service, html: { multipart: true } do |f| %>
  <%= f.label :first %>:
  <br>
  <%= f.text_field :first %><br />

  <%= f.label :last %>:
  <br>
  <%= f.text_field :last %><br />

  <%= f.label :number %>:
  <br>
  <%= f.text_field :number %><br />
  <br>
  <%= f.label :industry %>
  <br>
  <%= f.text_field :industry %>
  <br>
  <%= f.label :company %>
  <br>
  <%= f.text_field :company %>
  <br>
  <%= f.label :language %>
  <br>
  <%= f.text_field :language %>
  <br>
  <%= f.label :website %>
  <br>
  <%= f.text_field :website %>
  <br>
  <%= f.label :youtube %>
  <br>
  <%= f.text_field :youtube %>
  <br>
  <%= f.label :about %>
  <br>
  <%= f.text_field :about %>
  <br>
  <br>
  <%= f.label :image %>: Profile Photo
  <%= f.file_field :image %>
  <br>

  <%= f.submit %>
<% end %>
