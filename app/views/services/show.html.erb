<br>
<div class="container">
  <div class="col-md-4">
    <div class="well">
      <%= image_tag @service.image.url(:large), class: "img-responsive" %>
    </div>
    <div class="well">
      <p class="restaurant-title">
        <%= @service.first + " " + @service.last %>
      </p>
      <p class="restaurant-address">
        <%= @service.number %>
      </p>
      <p>
        <%= @service.language %>
      </p>
        <a href="<%= @service.website %>"><%= @service.website %></a>
      <br>
      <%= link_to "Edit", edit_service_path(@service) %>
      <br>
      <!-- <p>
        <%= link_to "Delete", food_path(@service), method: :delete, data: { confirm: "Are you sure you want to delete?"} %>
      </p> -->
    </div>
    <div class="well">
      <h4>Industry</h4>
        <%= @service.industry %>
    </div>
  </div>
  <div class="col-md-8">
    <div class="well">
      <div class="video-container">
      </div>
    </div>
    <div class="well">
      <h4>About</h4>
    </div>
    <div class="well">
      <h4>Tags</h4>
    </div>
  </div>
</div>
